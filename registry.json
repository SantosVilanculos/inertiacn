{
    "$schema": "https://ui.shadcn.com/schema/registry.json",
    "name": "inertiacn",
    "homepage": "https://inertiacn.vercel.app",
    "items": [
        {
            "name": "form",
            "type": "registry:component",
            "title": "Form",
            "description": "A form with context and  Zod validation and.",
            "dependencies": ["@inertiajs/core", "@inertiajs/react", "zod"],
            "registryDependencies": [],
            "files": [
                {
                    "path": "src/registry/new-york/ui/form.tsx",
                    "type": "registry:component"
                }
            ]
        },
        {
            "name": "use-route",
            "type": "registry:hook",
            "title": "useRoute",
            "description": "A route hook.",
            "dependencies": ["@inertiajs/react"],
            "registryDependencies": [],
            "files": [
                {
                    "path": "src/registry/new-york/hooks/use-route.tsx",
                    "type": "registry:hook"
                },
                {
                    "path": "src/registry/new-york/files/HandleInertiaRequests.php",
                    "type": "registry:file",
                    "target": "app/Http/Middleware/HandleInertiaRequests.php"
                }
            ]
        }
    ]
}
